{
  "name": "nest-auth-example",
  "version": "0.0.1",
  "description": "Nest.js authentication with Passport. Realworld example",
  "author": "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "fixtures": "fixtures-ts-node-commonjs load --require=dotenv-mono/load --require=tsconfig-paths/register --dataSource=./src/data-source.ts --ignoreDecorators test/fixtures",
    "start": "node dist/main.js",
    "start:dev": "NODE_OPTIONS='-r dotenv-mono/load' nest start --watch",
    "start:repl": "NODE_OPTIONS='-r dotenv-mono/load' nest start --entryFile repl",
    "start:debug": "NODE_OPTIONS='-r dotenv-mono/load' nest start --debug --watch",
    "migration:run": "typeorm migration:run -d dist/data-source",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "NODE_OPTIONS='-r dotenv-mono/load -r tsconfig-paths/register' typeorm-ts-node-commonjs -d src/data-source"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-pg-simple": "^9.0.0",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.6.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@jackfranklin/test-data-bot": "^2.0.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^29.2.4",
    "@types/node": "^20.1.0",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "dotenv-mono": "^1.3.10",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.0.0",
    "jest-ts-auto-mock": "^2.1.0",
    "node-mocks-http": "^1.12.1",
    "prettier": "^2.8.1",
    "supertest": "^6.3.2",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^28.0.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "ttypescript": "^1.5.13",
    "typeorm-fixtures-cli": "3.1.0",
    "typescript": "^4.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv-mono/load",
      "jest-ts-auto-mock"
    ],
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript",
        "tsconfig": "tsconfig.test.json"
      }
    }
  }
}
